<%= form_for @preference  do |f| %>
  <%= f.label :song_sort_order %>
  <%= f.text_field :song_sort_order%><br>
  <%= f.label :artist_sort_order %>
  <%= f.text_field :artist_sort_order %>
  <br />
  <%= f.label :allow_create_songs %>
   <%= f.checkbox_for :allow_create_songs %>
   <br>
   <%= f.label :allow_create_artists %>
   <%= f.checkbox_for :allow_create_artists %>
   <br>

  <%= f.submit %>
<% end %>
